OnCONVParse
{
_r = FOPEN("var/conv_zh.txt","r");
_script = FREAD("var/conv_zh.txt");
while _script!=-1
{
	"\0\s[0]%(_script)\n\n\w9\w9"
	_script = FREAD("var/conv_zh.txt");
	--
	"\1\s[10]%(_script)\n\n\w9\w9"
	_script = FREAD("var/conv_zh.txt");
	--
	"\0\s[0]%(_script)\n\n\w9\w9"
	_script = FREAD("var/conv_zh.txt");
	--
	"\1\s[10]%(_script)\n\n\w9\w9"
	_script = FREAD("var/conv_zh.txt");
}
_i = FCLOSE("var/conv_zh.txt");
}